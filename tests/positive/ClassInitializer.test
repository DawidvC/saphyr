
class Base
{
	struct this { int i; }

	this(int iVal)  i{iVal} { i = 4; }
}

class Base2
{
	struct this { int i; }

	this() i{4} {}
}

class Init
{
	struct this {
		int a;
		Base b;
		Base2 c;
	}

	this()
	a{4}, b{1}, c{}
	{ a = 3; }
}

========

%Base = type { i32 }
%Base2 = type { i32 }
%Init = type { i32, %Base, %Base2 }

define void @Base_this(%Base* %this, i32 %iVal) {
  %1 = alloca %Base*
  store %Base* %this, %Base** %1
  %2 = alloca i32
  store i32 %iVal, i32* %2
  %3 = load %Base** %1
  %4 = getelementptr %Base* %3, i32 0, i32 0
  %5 = load i32* %2
  store i32 %5, i32* %4
  %6 = load %Base** %1
  %7 = getelementptr %Base* %6, i32 0, i32 0
  store i32 4, i32* %7
  ret void
}

define void @Base2_this(%Base2* %this) {
  %1 = alloca %Base2*
  store %Base2* %this, %Base2** %1
  %2 = load %Base2** %1
  %3 = getelementptr %Base2* %2, i32 0, i32 0
  store i32 4, i32* %3
  ret void
}

define void @Init_this(%Init* %this) {
  %1 = alloca %Init*
  store %Init* %this, %Init** %1
  %2 = load %Init** %1
  %3 = getelementptr %Init* %2, i32 0, i32 0
  store i32 4, i32* %3
  %4 = load %Init** %1
  %5 = getelementptr %Init* %4, i32 0, i32 1
  call void @Base_this(%Base* %5, i32 1)
  %6 = load %Init** %1
  %7 = getelementptr %Init* %6, i32 0, i32 2
  call void @Base2_this(%Base2* %7)
  %8 = load %Init** %1
  %9 = getelementptr %Init* %8, i32 0, i32 0
  store i32 3, i32* %9
  ret void
}
